{% extends 'base.html.twig' %}

    {% block pageTitle %}Home{% endblock %}
    {% block body %}
        {% if nurls is defined %}
            {% for nurl in nurls %}
                <tr>
                    <td><a href="{{ path('nurl_show', { 'id': nurl.id }) }}">{{ nurl.id }}</a></td>
                    <td>{{ nurl.title }}</td>
                    <td>{{ nurl.link }}</td>
                    <td>{{ nurl.source }}</td>
                    <td>{{ nurl.note }}</td>
                    <td>
                        <ul>
                            <li>
                                <a href="{{ path('nurl_show', { 'id': nurl.id }) }}">show</a>
                            </li>
                            <li>
                                <a href="{{ path('nurl_edit', { 'id': nurl.id }) }}">edit</a>
                            </li>
                        </ul>
                    </td>
                </tr>
            {% endfor %}
        {% endif %}
    {% endblock %}